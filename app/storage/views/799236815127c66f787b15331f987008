    },
          subtitle: {
        text: 'Extras total:<?php foreach($serie_extra_total as $ex_total): ?> $<?php echo e(number_format($ex_total->total, 0, '.', ',')); ?><?php endforeach; ?>'

    },
    plotOptions: {
        pie: {
            allowPointSelect: true,
            cursor: 'pointer',
            dataLabels: {
                enabled: true,
                format: '<b>{point.name}</b>: ${point.y:,.0f}',
                style: {
                    color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                },
                connectorColor: 'silver'
            }
        }
    },
    series: [{
        name: 'Total',
        data: [
         <?php foreach($serie_extra as $ext): ?>
            { name: '<?php echo e($ext->nombre); ?>', y: <?php echo e($ext->monto); ?> },
               <?php endforeach; ?>            
        ]
    }]
});
