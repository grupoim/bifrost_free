    series: [{
        name:'Atrasado', 
        data: [<?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 31 a 60 dias' and $sc->month == $fechas['month3'] and $sc->year == $fechas['year']): ?><?php $sum3 = $sc->monto  ?> <?php else: ?> <?php $sum3 = 0;?> <?php endif; ?> <?php endforeach; ?> 
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 61 a 90 dias' and $sc->month == $fechas['month3'] and $sc->year == $fechas['year']): ?><?php $sum2 = $sc->monto + $sum3; ?><?php else: ?> <?php $sum2 = 0;?> <?php endif; ?> <?php endforeach; ?>
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 91 a 120 dias' and $sc->month == $fechas['month3'] and $sc->year == $fechas['year']): ?><?php $sum_atrasado3 = $sc->monto + $sum2; ?>  <?php echo e(round($sum_atrasado3,0)); ?>, <?php endif; ?> <?php endforeach; ?>  
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 31 a 60 dias' and $sc->month == $fechas['month2'] and $sc->year == $fechas['year']): ?><?php $sum3 = $sc->monto  ?> <?php else: ?> <?php $sum3 = 0;?> <?php endif; ?> <?php endforeach; ?> 
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 61 a 90 dias' and $sc->month == $fechas['month2'] and $sc->year == $fechas['year']): ?><?php $sum2 = $sc->monto + $sum3; ?> <?php else: ?> <?php $sum2 = 0;?> <?php endif; ?> <?php endforeach; ?>
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 91 a 120 dias' and $sc->month == $fechas['month2'] and $sc->year == $fechas['year']): ?><?php $sum_atrasado2 = $sc->monto + $sum2; ?>  <?php echo e(round($sum_atrasado2,0)); ?>, <?php endif; ?> <?php endforeach; ?>  
               <?php foreach($serie_cartera as $sc): ?> <?php if($sc->nombre == 'Atrasado 31 a 60 dias' and $sc->month == $fechas['month'] and $sc->year == $fechas['year']): ?><?php $sum3 = $sc->monto  ?><?php else: ?> <?php $sum3 = 0;?> <?php endif; ?> <?php endforeach; ?> 
