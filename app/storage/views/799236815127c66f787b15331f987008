Highcharts.chart('Cartera_pastel', {

    chart: {
        plotBackgroundColor: null,
        plotBorderWidth: null,
        plotShadow: false,
        type: 'pie'
    },
    title: {
        text: 'Cartera clientes <?php echo e($mes); ?> <?php echo e($serie['name']); ?>'
    },

    tooltip: {
           pointFormat: '{series.name}: ${point.y:,.0f}<br/>'
    },
    subtitle: {
          text: 'Cartera total:<?php foreach($serie_cartera_total as $car_total): ?> $<?php echo e(number_format($car_total->total, 0, '.', ',')); ?><?php endforeach; ?>'
    },
    plotOptions: {
        pie: {
            allowPointSelect: true,
            cursor: 'pointer',
            dataLabels: {
                enabled: true,
               format: '<b>{point.name}</b>: {point.percentage:.1f} %',
                style: {
                    color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                },
                connectorColor: 'silver'
            }
        }
    },
    series: [{
        name: 'Total',
        data: [<?php foreach($serie_cartera as $d): ?> <?php if($d->month == $fechas['month'] and $d->year == $fechas['year']): ?> { name:'<?php echo e($d->nombre); ?>', y: <?php echo e($d->monto); ?> }, <?php endif; ?> <?php endforeach; ?>]

    }]
});
