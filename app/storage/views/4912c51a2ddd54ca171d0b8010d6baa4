<script>

  $(document).on('ready', function(){


  	 window.setTimeout(function() {
  $(".alerta").fadeTo(500, 0).slideUp(500, function(){
      $(this).remove();
  
});

	$("#folio_venta").show();
	$("#pieza_entera").show();
  $(".select_pieza_entera_p").hide();
  $("#pieza_entera_e").hide();
	$(".corte_especial").hide();
  $(".area_uso_p").hide();
  $("#").hide();
	$(".stock").show();
	$("#motivo_de_reposicion").hide();
	$("#cantidad_stock").hide();
	$("#password").hide(); 
  $(".select_pieza_especial").hide(); 	
	
//venta
    $("#r1").click(function () {
        $("#motivo_de_reposicion").hide();
        $("#password").hide();
        $("#folio_venta").show();
        $("#cantidad_stock").hide();
          
    });

//reposicion
    $("#r2").click(function () {
        $("#motivo_de_reposicion").show();
        $("#password").show();
       	$("#folio_venta").hide();
       	$("#cantidad_stock").hide();
        
    });
     
//stock
     $("#r3").click(function () {
       $("#motivo_de_reposicion").hide();
       $("#password").hide();
       $("#folio_venta").hide();
       $("#cantidad_stock").show();
    });

      $("#r1corte").click(function () {
       $("#pieza_entera").show();
       $(".pieza_entera").show();
       $(".corte_especial").hide();              
       
    });
       
      $("#r2corte").click(function () {
       $("#pieza_entera").hide();
       $(".pieza_entera").hide();
       $(".corte_especial").show();
       $(".stock").hide();
       $(".stock").disable();
       
    });

       $("#r3corte").click(function () {
       $("#pieza_entera").show();
       $(".select_pieza_estandar").show();
       $(".select_pieza_especial").hide();
       $(".area_uso_p").hide();
      
       
    });

       $("#r4corte").click(function () {
       $(".select_pieza").hide();
       $(".select_pieza_estandar").hide();
       $(".select_pieza_especial").show();
       $(".area_uso_p").show();
      
       
    });
       });

  	
     

    $(".proveedor").chosen({   
    no_results_text: "No hay resultados para:",        
    width: "100%"    
  });

    $(".material_color").chosen({   
    no_results_text: "No hay resultados para:",    

    width: "100%"    
  });

  });
  </script>
 

